{
  "name": "presearch",
  "displayName": "Presearch",
  "description": "Presearch for answers to any error, mistakes, or confusion you may have!",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:presearch.makeSearch",
    "onCommand:presearch.makeSearchFromTerminal"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "presearch.makeSearch",
        "title": "presearch selected text"
      }
    ],
    "keybindings": [
      {
        "command": "presearch.makeSearch",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "when": "editorHasSelection"
      },
      {
        "command": "presearch.makeSearchFromTerminal",
        "key": "ctrl+alt+p o",
        "mac": "cmd+alt+p o",
        "when": "terminalFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "presearch.makeSearch",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "publisher": "Ishan-Ravindu",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ishan-Ravindu/presearch-VScode.git"
  }
}
